<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui" >


<ui:style>

	.widthHeight100 {
		width: 100%;
		height: 100%;
	}
	
</ui:style>


<g:HTMLPanel addStyleNames="{style.widthHeight100}" >

	<div ui:field="mapContainer"
	     class="{style.widthHeight100}" >
	</div>

</g:HTMLPanel>


</ui:UiBinder> 